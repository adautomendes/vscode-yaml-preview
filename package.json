{
    "name": "yaml-preview",
    "displayName": "Yaml Preview",
    "description": "Displays a preview of yaml content",
    "version": "0.0.1",
    "publisher": "daiwata",
    "license": "MIT",
    "keywords": [
        "yaml",
        "preview",
        "markup",
        "viewer",
        "render"
    ],
    "engines": {
        "vscode": "^1.41.1"
    },
    "activationEvents": [
        "onCommand:yaml.showPreview",
        "onCommand:yaml.showPreviewToSide",
        "onLanguage:yaml"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "yaml.showPreview",
                "title": "Open preview",
                "category": "Yaml"
            },
            {
                "command": "yaml.showPreviewToSide",
                "title": "Open preview to the side",
                "category": "Yaml"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^7.10.6",
        "typescript": "^2.9.2",
        "vscode": "^1.1.34"
    },
    "dependencies": {
        "js-yaml": "^3.13.1"
    }
}